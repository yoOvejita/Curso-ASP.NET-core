<table>
    <tr>
        <td>ID</td>
        <td>Nombre</td>
        <td>Precio</td>
        <td>Imagen</td>
        <td>Opciones</td>
    </tr>
    @foreach (var prod in ViewBag.productitos)
    {
        <tr>
            <td>@prod.id</td>+
            <td>@prod.nombre</td>
            <td>@prod.precio</td>
            <td>@prod.imagen</td>
            <td><a asp-controller="Clase12" asp-action="Editar" asp-route-id="@prod.id">Editar</a> 
                <a asp-controller="Clase12" asp-action="Eliminar" asp-route-id="@prod.id">Eliminar</a></td>
        </tr>
    }

</table>
<a asp-action="Otro" asp-controller="Clase11">AGREGAR NUEVO</a>